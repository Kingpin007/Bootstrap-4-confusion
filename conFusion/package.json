{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "Anirudh Kanabar",
  "license": "MIT",
  "devDependencies": {
    "lite-server": "^2.2.2",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kingpin007/Bootstrap-4-confusion.git"
  },
  "bugs": {
    "url": "https://github.com/Kingpin007/Bootstrap-4-confusion/issues"
  },
  "homepage": "https://github.com/Kingpin007/Bootstrap-4-confusion#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper": "^1.0.1"
  }
}
